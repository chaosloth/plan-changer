# Job configuration
JOB_NAME=plan-changer-job
LOCK_DIR=/tmp
LAUNTEL_DEBUG_HTML=1

# Launtel configuration
LAUNTEL_BASE=https://residential.launtel.net.au
LAUNTEL_USERNAME=your_username_here
LAUNTEL_PASSWORD=your_password_here

# Required identifiers (from your Launtel account/service)
LAUNTEL_USERID=your_user_id
LAUNTEL_SERVICE_ID=your_service_id
LAUNTEL_AVCID=your_avc_id
LAUNTEL_LOCID=your_loc_id

# Optional parameters (leave blank if not used)
LAUNTEL_DISCOUNT_CODE=
LAUNTEL_UNPAUSE=0
LAUNTEL_COAT=0
LAUNTEL_CHURN=0
LAUNTEL_SCHEDULEDDT=
LAUNTEL_NEW_SERVICE_PAYMENT_OPTION=

# Runtime
LAUNTEL_TIMEOUT_MS=15000
